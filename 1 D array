<script>
  //one dimensional array
  //Create a function called highestScore that will
  //1. recieve a 1d array called scores
  //2. return the highest score
 

  function highestScore(scores) {
   // console.log(scores);
 var max = scores[0]; //1st index take max dhore nilam
 for (var x=1; x<scores.length; x++){
 if (max < scores[x]){
  max = scores[x];
      }
 }
 return max;
    }
  var scores = [21,28,1,88,15];//hightstscore er moddhe pass kore dite hbe
 var maxscore = highestScore(scores);
console.log(maxscore);
</script>
   
