//initialy sum = 0, x=1, jeta sum
//0 er sathe jog hbe. then x=2 jeta
// sum 1 er sathe jog hbe. ebhabe cholbe.

var m=parseInt(prompt("enter the starting number :"));
var n=parseInt(prompt("enter the last number:"));


var sum = 0;
    for(var x=m; x<=n; x=x+1){
        sum=sum + x;

}
document.write(sum);



